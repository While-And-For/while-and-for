{
  "name": "@while-and-for/components",
  "private": false,
  "version": "0.0.4-4",
  "description": "Common components wrapper for antd",
  "keywords": [
    "antdesign",
    "components",
    "react"
  ],
  "author": "Regan Langford <regan.reihana@gmail.com>",
  "homepage": "https://github.com/While-And-For/while-and-for#readme",
  "license": "ISC",
  "main": "dist/index.js",
  "types": "dist/index",
  "directories": {
    "test": "__tests__",
    "dist": "dist"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
